    <script>
        var startX,//触摸时的坐标
          startY,
          x, //滑动的距离
          y,
          aboveY = 0; //设一个全局变量记录上一次内部块滑动的位置


        // var inner1 = document.getElementById("inner1");
        //   var inner1 = $(".container");

        function touchSatrt(e) {//触摸
            var touch = e.touches[0];
            startY = touch.pageY; //刚触摸时的坐标
        }
        function touchMove(e) {//滑动
            var touch = e.touches[0];
            y = touch.pageY - startY;//滑动的距离
            //inner.style.webkitTransform = 'translate(' + 0+ 'px, ' + y + 'px)'; //也可以用css3的方式
            //  inner1.style.left = aboveX1 + x1 + "px"; //这一句中的
            var juli =( aboveY + y);
            $(".container").css({
                "-webkit-transform": "rotateX(" + juli + "deg) rotateY(0deg)",
                "transform": "rotateX(" + juli + "deg) rotateY(0deg)",
            });
            $(".container").attr("data-data", juli)
        }

        function touchEnd(e) {//手指离开屏幕
            aboveY = parseInt($(".container").attr("data-data"));
            // aboveX1 = parseInt(inner1.style.left);//touch结束后记录内部滑块滑动的位置 在全局变量中体现 一定要用parseInt()将其转化为整数字;

        }//
        document.getElementById("outer1").addEventListener('touchstart', touchSatrt, false);
        document.getElementById("outer1").addEventListener('touchmove', touchMove, false);
        document.getElementById("outer1").addEventListener('touchend', touchEnd, false);
    </script>



</body>
</html>

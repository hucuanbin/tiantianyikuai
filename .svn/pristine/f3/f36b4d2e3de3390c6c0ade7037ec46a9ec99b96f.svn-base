#jswbox {
    width: 1100px;
    height: 600px;
    margin: 0 auto;
    position: relative;
}

    #jswbox ul {
        position: relative;
        height: 600px;
    }

    #jswbox li {
        position: absolute;
        width: 0;
        height: 0;
        z-index: 0;
        cursor: pointer;
        overflow: hidden;
        top: 152px;
        left: 10px;
    }

        #jswbox li img {
            width: 100%;
            height: 100%;
            vertical-align: top;
            float: left;
        }

    #jswbox .prev, #jswbox .next {
        display: none;
    }

.container-fluid {

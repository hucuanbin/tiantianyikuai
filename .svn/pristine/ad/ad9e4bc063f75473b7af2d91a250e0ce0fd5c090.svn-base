    <script>
        //添加购物车

        $(".gwc_tj").click(function (e) {
            var src = $(this).parent().parent().find(".sp_Name").attr("src");
            var a_w = $(this).parent().parent().find(".sp_Name").offset().top;
            var a_l = $(this).parent().parent().find(".sp_Name").offset().left;
            var a1_w = $(".index_gwx_wq").offset().top;
            var a1_l = $(".index_gwx_wq").offset().left;
            //    alert("TOP:"+a1_w+",left"+a1_l)
            //  alert(gwc.offset().top);
            $(".wohuifei>img").attr("src", src);
            $(".wohuifei").css("display", "inline-block");
            $(".wohuifei").css("top", a_w);
            $(".wohuifei").css("left", a_l);


            var time1 = setTimeout(
                function () {
                    $(".wohuifei>img").css("box-shadow", "0px 0px 15px #999")
                    $(".wohuifei>img").css("transition", "0.6s linear");
                    $(".wohuifei>img").css("border-radius", "50px");
                    $(".wohuifei>img").css("border", "1px #ccc solid");

                }, 70);
            var time2 = setTimeout(
                function () {
                    $(".wohuifei").css("transition", "0.4s linear");
                    $(".wohuifei>img").css("transition", "0.4s linear");
                    $(".wohuifei>img").css("height", "35px");
                    $(".wohuifei>img").css("width", "35px");
                    $(".wohuifei").css("height", "35px");
                    $(".wohuifei").css("width", "35px");
                    $(".wohuifei").css("top", a1_w);
                    $(".wohuifei").css("left", a1_l);
                }, 970);
            var time3 = setTimeout(
                  function () {
                      $(".wohuifei").css("display", "none");
                      $(".wohuifei").attr("style", "");
                      $(".wohuifei>img").attr("style", "");
                      $(".xuan_fuc>span").attr("class", "atc");
                      var shuzi = parseInt($(".index_gwx_nei").text());
                      $(".index_gwx_nei").text(shuzi + 1);
                  }, 1400);
            var time4 = setTimeout(
                        function () {
                            $(".xuan_fuc>span").attr("class", "");
                         
                        }, 2450);


            var pid = $(this).attr("id").substr(13);
            add_shoppingcart(pid);
        })

    </script>

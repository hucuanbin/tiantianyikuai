    <script>
    $("body").height($(window).height());

    $(".top_tb_1").click(function () {
        var a = $(".wp_index_left").css("left");
        if (a != "0px") {
            $(".wangp_lieb").addClass("shou");
        }
    });
    $(".wp_index_rigt").click(function () {

        var a = $(".wp_index_left").css("left");
        if (a == "0px") {
            $(".wangp_lieb").attr("class", "wangp_lieb");
        }

    })
    </script>




</body>
</html>

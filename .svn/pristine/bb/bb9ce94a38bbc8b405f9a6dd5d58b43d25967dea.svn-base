    <script>

        //出现回到顶部图标
        $(window).scroll(function () {
            var st = parseInt($(this).scrollTop());
            if (st > 100) {
                $(".index_goon").fadeIn(300);
            }
            else {
                $(".index_goon").fadeOut(300);
            }
        });

        //点击回到顶部图标
        $(".index_goon").click(function () {
            $('html,body').animate({ scrollTop: '0px' }, 300);
        })


